apiVersion: v1
kind: Service
metadata:
  name: newpod-svc
spec:
  type: NodePort
  selector:            <-----------------------<<< SELECTOR
    app: myapp
  ports:
    - port: 80
      protocol: TCP
      targetPort: 80
      nodePort: 30122

apiVersion: v1
kind: Pod
metadata:
  name: newpod
  labels:             <-----------------------<<< LABEL
    app: myapp
spec:
  containers:
    - name: con1
      image: nginx:latest
      ports:
        - containerPort: 80
